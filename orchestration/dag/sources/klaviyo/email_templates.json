{
  "stream": "email_templates",
  "options": {
    "sync_modes": [
      "full_refresh",
      "incremental"
    ],
    "destination_sync_modes": [
      "overwrite",
      "append",
      "append_dedup"
    ]
  },
  "defaults": {
    "primary_key": [
      [
        "id"
      ]
    ],
    "cursor_field": [
      "updated"
    ]
  },
  "fields": {
    "links": {
      "type": "object",
      "properties": {
        "self": {
          "type": "string"
        }
      }
    },
    "attributes": {
      "type": "object",
      "properties": {
        "company_id": {
          "type": "string"
        },
        "created": {
          "type": "string",
          "format": "date-time"
        },
        "name": {
          "type": "string"
        },
        "html": {
          "type": "string"
        },
        "text": {
          "type": "string"
        },
        "updated": {
          "type": "string",
          "format": "date-time"
        },
        "editor_type": {
          "type": "string"
        }
      }
    },
    "id": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "updated": {
      "type": "string",
      "format": "date-time"
    }
  }
}